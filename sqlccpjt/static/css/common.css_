.lay_pop .pop_cont .pop_search :-ms-input-placeholder{font-size:13px!important; color:#888!important; opacity:1;}
.lay_pop .pop_cont .pop_search .btn{position:absolute; top:0; right:0;}
.show_hide_pop_server{width:800px; height:600px; margin:-300px 0 0 -400px;}

