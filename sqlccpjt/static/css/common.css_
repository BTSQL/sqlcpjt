/* 등록안됨 */ 
.empty{display:table-cell; width:1100px; height:480px; border:1px solid #40465C; vertical-align:middle; box-sizing:border-box;}
.empty strong{display:block; padding:140px 0 22px 0; margin-top:-29px; text-align:center; font-size:29px; color:#468ed5; font-weight:normal; background:url(../images/icon_empty.gif) no-repeat center 0;}
.empty p{text-align:center; font-size:15px; line-height:26px; color:#c9cddf;}

/* tap */
.tap_wrap .tap_btn{margin-bottom:-1px;}
.tap_wrap .tap_btn:after{content:""; display:block; clear:both;} 
.tap_wrap .tap_btn li{float:left; margin-right:-1px;}
.tap_wrap .tap_btn li a{display:block; height:48px; line-height:48px; padding:0 30px; box-sizing:border-box; border:1px solid #42475c; border-radius:5px 5px 0 0; font-size:19px; color:#82879d;}
.tap_wrap .tap_btn li.on a{background:#3f71bc; border:1px solid #3f71bc;  color:#fff;}
.tap_wrap .tap_content{position:absolute; top:-1000em;}
.tap_wrap .tap_btn+.tap_content{position:relative; top:0;}
.tap_wrap .right_btns{margin-bottom:10px;}
.tap_wrap .tap_content .border_line{padding:20px; border:1px solid #42475c;}

/* 레이어 팝업 */
.lay_pop{display:none; position:absolute; top:50%; left:50%; z-index:1000; background:#252a3d; border-radius:5px;}
